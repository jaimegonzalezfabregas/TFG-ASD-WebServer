                    <div class="container-fluid below-navbar px-4">
                        <div class="card mb-4">
                            <div class="card-header">
                                <i class="fas fa-table me-1"></i>
                                Profesores con infracciones
                            </div>
                            <div class="card-body">
                                <table id="datatablesSimple">
                                    <thead>
                                        <tr>
                                            <th>Nombre</th>
                                            <th>Faltas totales</th>
                                            <th>Faltas en el último mes</th>
                                            <th>Faltas justificadas</th>
                                        </tr>
                                    </thead>
                                    <tfoot>
                                        <tr>
                                            <th>Nombre</th>
                                            <th>Faltas totales</th>
                                            <th>Faltas en el último mes</th>
                                            <th>Faltas justificadas</th>
                                        </tr>
                                    </tfoot>
                                    <tbody>
                                        <% docentes.forEach(function(docente) { %>
                                            <tr>
                                                <td>docente.nombre</td>
                                                <td>docente.totales</td>
                                                <td>docente.mes</td>
                                                <td>docente.justificadas</td>
                                            </tr>
                                        <% }); %>
